#admin-low-zoom,
#admin-mid-zoom,
#admin-high-zoom {
  [admin_level = '2'],
  [admin_level = '3'] {
    line-clip: false;
    line-color: darken(#0A2124, 5%);
    line-width: 4;
  }

  [admin_level = '4'] {
    line-clip: false;
    line-color: darken(#0A2124, 5%);
    line-width: 2;
  }

//  opacity: 0.4;
//  comp-op: darken;
}